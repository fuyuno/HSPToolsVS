<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
  <TemplateData>
    <Name>HSP3Dish Smartphone Application</Name>
    <Description>A project for creating a new HSP3Dish smartphone application.</Description>
    <Icon>HSP3DishApplication.png</Icon>
    <ProjectType>HSP</ProjectType>
    <SortOrder>20</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>HSP3DishSmartphoneApplication</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <CreateInPlace>true</CreateInPlace>
    <PromptForSaveOnCreation>true</PromptForSaveOnCreation>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="$safeprojectname$.hsproj" File="HSP3DishApplication.hsproj" ReplaceParameters="true">
      <ProjectItem OpenInEditor="true">Program.hsp</ProjectItem>
      <ProjectItem>hsp3dish.ini</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>

